<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#2a2a2a"
    tools:context=".Equipes">

    <ImageButton
        android:id="@+id/imageButton5"
        android:layout_width="153dp"
        android:layout_height="153dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="108dp"
        android:layout_marginEnd="71dp"
        android:layout_marginBottom="20dp"
        android:background="@color/black_500"
        android:onClick="telaFormatadores"
        android:scaleType="centerInside"
        android:src="@drawable/formatadores_"
        app:layout_constraintBottom_toTopOf="@+id/imageButton"
        app:layout_constraintEnd_toStartOf="@+id/imageButton2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageButton
        android:id="@+id/imageButton3"
        android:layout_width="153dp"
        android:layout_height="149dp"
        android:layout_marginStart="72dp"
        android:layout_marginEnd="16dp"
        android:layout_marginBottom="170dp"
        android:onClick="telaMecapeta"
        android:scaleType="centerCrop"
        android:src="@drawable/logo_mecapeta"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/imageButton"
        app:layout_constraintTop_toBottomOf="@+id/imageButton2" />

    <ImageButton
        android:id="@+id/imageButton"
        android:layout_width="153dp"
        android:layout_height="149dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="368dp"
        android:layout_marginEnd="1dp"
        android:layout_marginBottom="214dp"
        android:onClick="telaJogostosos"
        android:scaleType="centerCrop"
        android:src="@drawable/logo_jogostosos"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/imageButton3"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageButton
        android:id="@+id/imageButton2"
        android:layout_width="153dp"
        android:layout_height="149dp"
        android:layout_marginStart="240dp"
        android:layout_marginTop="108dp"
        android:layout_marginEnd="18dp"
        android:layout_marginBottom="20dp"
        android:onClick="telaDST"
        android:src="@drawable/dst_"
        app:layout_constraintBottom_toTopOf="@+id/imageButton3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>